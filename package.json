{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "yarn js-build && eleventy && yarn test",
    "start": "concurrently \"eleventy --serve\" \"yarn js-build-watch\" \"yarn test-watch\"",
    "serve": "eleventy --serve",
    "js-build": "./scripts/js-makefile",
    "js-build-watch": "yarn js-build --watch",
    "persist-images": "./scripts/persist-images",
    "generate-md": "./scripts/docs-generate",
    "debug": "DEBUG=* eleventy && yarn test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "eleventy-plugin-local-images": "^0.4.0",
    "esbuild": "^0.8.52",
    "file-type": "^16.2.0",
    "fs-extra": "^9.1.0",
    "jsdom": "^16.4.0",
    "luxon": "^1.21.3",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.7.5",
    "concurrently": "^6.0.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "expect.js": "^0.3.1",
    "gorko": "^0.2.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^4.1.0",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.4",
    "jimp": "^0.16.1",
    "lru-cache": "^6.0.0",
    "mocha": "^8.1.3",
    "phin": "^3.5.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.2",
    "querystring": "^0.2.0",
    "sass": "^1.32.8",
    "sharp": "^0.27.2",
    "twitter": "^1.7.1"
  }
}
